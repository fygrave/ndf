<div style="z-index:100000;cursor: pointer; position:absolute;left:0px;height:220px;width:30px;background:url('/nm2008/i/video_left_f.png');display:none;" id="mr_bl_click"><!-- --></div>
<script>
var videoend_bl = 0;
var colv=25;
$("#ml_bl_click").click(function(){
if (videoend_bl != 1){
videoend_bl = 1;
$("#left_bl_l").animate({"margin-left": "-=244px"}, "slow", function () {
videoend_bl = 0;
});
a = ($("#left_bl_l").css("margin-left"));
b = 244*(colv-3)-20;
if (a == ('-' + b + 'px')){
$("#ml_bl_click").css('display','none');
}
if (a == ('20'+'px')){
$("#mr_bl_click").css('display','block');
}
}
});
$("#mr_bl_click").click(function(){
if (videoend_bl != 1){
videoend_bl = 1;
$("#left_bl_l").animate({"margin-left": "+=244px"}, "slow", function () {
videoend_bl = 0;
});
a = ($("#left_bl_l").css("margin-left"));
b = 244*(colv-2)-20;
if (a == ('-' + b + 'px')){
$("#ml_bl_click").css('display','block');
}
if (a == ('-'+'224'+'px')){
$("#mr_bl_click").css('display','none');
}
}
});
</script>
