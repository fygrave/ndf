<script>
function news_show_tab(t,show) {
news_hide_tabs();
document.getElementById(show).style.display = "block";
document.getElementById('li_'+t).className += " current";
}
function news_hide_tabs()
{
if(document.getElementById('t')) document.getElementById('t').style.display = "none";
if(document.getElementById('m')) document.getElementById('m').style.display = "none";
if(document.getElementById('w')) document.getElementById('w').style.display = "none";
if(document.getElementById('li_tab1')) document.getElementById('li_tab1').className = document.getElementById('li_tab1').className.replace(/ +current/, '');
if(document.getElementById('li_tab2')) document.getElementById('li_tab2').className = document.getElementById('li_tab2').className.replace(/ +current/, '');
if(document.getElementById('li_tab3')) document.getElementById('li_tab3').className = document.getElementById('li_tab3').className.replace(/ +current/, '');
}
</script>
