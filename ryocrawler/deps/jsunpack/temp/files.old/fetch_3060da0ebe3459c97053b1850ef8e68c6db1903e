function news_show1_tab(t,show1) {
news_hide1_tabs();
document.getElementById(show1).style.display = "block";
document.getElementById('li_'+t).className += " current";
}
function news_hide1_tabs()
{
if(document.getElementById('t2')) document.getElementById('t2').style.display = "none";
if(document.getElementById('m2')) document.getElementById('m2').style.display = "none";
if(document.getElementById('w2')) document.getElementById('w2').style.display = "none";
