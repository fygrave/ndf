<script language="javascript">
function setCurDate() {
    var t,re,a,y,m,d;
    t=new Date();
    re=new RegExp("/([0-9]+)/([0-9]+)/([0-9]+)");
    a=re.exec(document.location.href);
    if(a==null) {
  y=t.getFullYear();
  m=t.getMonth()+1; m=(m<10?'0'+m:m);
  d=t.getDate();    d=(d<10?'0'+d:d);
    } else {
  y=a[1]; m=a[2]; d=a[3];
    }
    document.fcal.y.value=y;
    document.fcal.m.value=m;
    document.fcal.d.value=d;
}
</script>
